{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "./log/photobooth-log.json",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "Application": "PhotoboothV1",
      "Environment": "Development"
    }
  },
  "PhotoBoothSettings": {
    "SerialPortName": "/dev/ttyTHS1",
    "SerialPortBaudRate": "9600",
    "SerialPortDtrEnable": "TRUE",
    "StoragePath": "photobooth",
    "CollageGridWidth": "2",
    "CollageGridHeight": "2",
    "CollageScalePercentage": "1.0",
    "CollagePaddingPixels": "15",
    "CollageAspectRatio": "1.47",
    "Filter": "Original",
    "PrinterName": "QRCode",
    "PrintMarginTop": "6",
    "PrintMarginLeft": "0",
    "PrintMarginBottom": "0",
    "PrintMarginRight": "6",
    "PrintDurationMs": "2000",
    "SaveSessions": "true",
    "OffloadSessions": "true",
    "EventId": "FuifVeerle12",
    "OffloadAddress": "https://photobooth.itsmyparty.nl",
    "FixedImageFilename": "",
    "OverlayImageFilename": "",
    "ApiEventId": "6676e1ca5a405e7dd3492168"
  },
  "AllowedHosts": "*"
}